<p id="notice"><%= notice %></p>

<h1>Search Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @search_images.each do |search_image| %>
      <tr>
        <td><%= link_to 'Show', search_image %></td>
        <td><%= link_to 'Edit', edit_search_image_path(search_image) %></td>
        <td><%= link_to 'Destroy', search_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Search Image', new_search_image_path %>
